                if (!a.isPinned && b.isPinned)
                    return 1;
                // ÁΩÆÈ°∂Áä∂ÊÄÅÁõ∏Âêå,ÊåâÊó∂Èó¥ÂÄíÂ∫è
                return b.createTime - a.createTime;
            });
            this.applyFilter();
        }
        catch (error) {
            this.diaryList = [];
            this.filteredList = [];
        }
