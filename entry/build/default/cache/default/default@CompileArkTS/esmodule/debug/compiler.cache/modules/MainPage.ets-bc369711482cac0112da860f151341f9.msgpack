    Row({ space: 4 }) {
      // ÁΩÆÈ°∂ÊåâÈíÆ
      Button() {
        Column() {
          Image($r('app.media.icon'))
            .width(20)
            .height(20)
            .fillColor('#FFFFFF')
          Text(diary.isPinned ? 'ÂèñÊ∂à' : 'ÁΩÆÈ°∂')
            .fontSize(11)
            .fontColor('#FFFFFF')
            .margin({ top: 4 })
        }
      }
      .width(70)
      .height('100%')
      .backgroundColor('#FF9800')
      .type(ButtonType.Normal)
      .onClick(() => this.togglePin(diary))

      // Âà†Èô§ÊåâÈíÆ
      Button() {
        Column() {
          Image($r('app.media.icon'))
            .width(20)
            .height(20)
            .fillColor('#FFFFFF')
          Text('Âà†Èô§')
            .fontSize(11)
            .fontColor('#FFFFFF')
            .margin({ top: 4 })
        }
      }
      .width(70)
      .height('100%')
      .backgroundColor('#FF5252')
      .type(ButtonType.Normal)
      .onClick(() => this.deleteDiary(diary, index))
    }
    .height('100%')
  }
